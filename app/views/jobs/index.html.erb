
  <table class="table table-boldered table-striped">
  <thead>
    <tr>
    <th>Job Title</th>
    <th>Job Description</th>
    <th>Created_at</th>
    </tr>
  </thead>
  <tbody>
    <%  @jobs.each do |job|%>
    <tr>
      <td><%=link_to(job.title,job_path(job))%></td>
      <td><%=job.description %></td>
      <td><%=job.created_at %></td>
    </tr>
    <% end %>
  </tbody>
