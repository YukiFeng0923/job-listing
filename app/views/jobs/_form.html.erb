<%= simple_form_for @job do |f| %>
<%=f.input :title, label: :"职位名称"%>
<%=f.input :description, label: :"职位描述"%>
<%= f.input :wage_upper_bound, label: :"薪资上限"%>
<%= f.input :wage_lower_bound,label: :"薪资上下限"%>
<%= f.input :contact_email, label: :"联系邮件"%>
<%= f.input :is_hidden,label: :"是否隐藏？"%>
<%=f.submit "Submit"  %>
<% end %>
